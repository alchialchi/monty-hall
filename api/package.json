{
  "name": "api",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node app.ts",
    "watch": "nodemon app.ts",
    "test": "jest"
  },
  "dependencies": {
    "@jest/types": "^26.6.2",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "jest": "^26.6.3"
  }
}
